{
    "version":2,
    "builds":[
        {
        "src":"index.js",
        "use":"@vercel/node"
    }
    ]
    ,
    "routes":[
        {
            "src":"/(.*)",
            "dest":"app.js"

        }
    ]
,  
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "access-control-allow-origin",
            "value": "*"
          }
        ]
      }
    ]
    
  
}


